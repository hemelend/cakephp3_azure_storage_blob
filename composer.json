{
    "name": "hemelend/cakephp3_azure_storage_blob",
    "description": "Azure Storage Blob datasource in CakePHP3",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
      "cakephp3",
      "Azure",
      "Storage Blob",
      "datasource"
    ],
    "homepage": "https://github.com/hemelend/cakephp3_azure_storage_blob.git",
    "authors": [
      {
        "name": "hemelend"
      }
    ],
    "minimum-stability": "dev",
    "require": {
      "microsoft/azure-storage-blob": "*"
    },
    "require-dev": {
      "phpunit/phpunit": "^5|^6",
      "cakephp/cakephp": "3.7.*",
      "cakephp/cakephp-codesniffer": "^3.0",
      "mockery/mockery": "dev-master"
    },
    "autoload": {
      "psr-4": {
        "CakeAzureStorageBlob\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "CakeAzureStorageBlob\\Test\\": "tests"
      }
    },
    "scripts": {
      "test": "phpunit"
    }
  }